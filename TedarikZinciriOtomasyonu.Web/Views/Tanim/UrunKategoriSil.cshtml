@model TedarikZinciriOtomasyonu.VarlikKatmani.urunkategori

@{
	ViewBag.Title = "Ürün Kategorisi Sil";
}

<section class="content-header">
	<div class="header-icon">
		<i class="fa fa-users"></i>
	</div>
	<div class="header-title">
		<h1>Ürün Kategorileri</h1>
		<small>Ürün Kategorisi Sil</small>
	</div>
</section>
<section class="content">
	<div class="row">
		<div class="col-sm-12">
			<div class="panel panel-bd lobidrag">
				<div class="panel-heading">
					<div class="btn-group" id="buttonlist">
						<a class="btn btn-add " href="~/Tanim/UrunKategorileri">
							<i class="fa fa-list"></i>  Ürün Kategorileri Listesi
						</a>
					</div>
				</div>
				<div class="panel-body">
					<dl class="dl-horizontal">
						<dt>
							@Html.DisplayNameFor(model => model.UrunKategoriAdi)
						</dt>

						<dd>
							@Html.DisplayFor(model => model.UrunKategoriAdi)
						</dd>

						<dt>
							@Html.DisplayNameFor(model => model.UrunKategoriAciklama)
						</dt>

						<dd>
							@Html.DisplayFor(model => model.UrunKategoriAciklama)
						</dd>

					</dl>

					@using (Html.BeginForm())
					{
						@Html.AntiForgeryToken()

						<div class="form-actions no-color">
							<input type="submit" value="Sil" class="btn btn-default" />
						</div>
					}
				</div>
			</div>
		</div>
	</div>
</section>

